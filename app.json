{
  "expo": {
    "name": "loyaltyjo scanner",
    "slug": "loyaltyjo-scanner",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "android": {
      "package": "com.anonymous.boltexponativewind",
      "versionCode": 1,
      "permissions": [
        "CAMERA"
      ]
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "66ae69f9-af76-48a2-b492-60910edcffee"
      }
    },
    "owner": "mayank_1734"
  }
}
